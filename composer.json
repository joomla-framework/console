{
    "name": "joomla/console",
    "type": "joomla-package",
    "description": "Joomla Console Package",
    "keywords": ["joomla", "framework", "console"],
    "homepage": "https://github.com/joomla-framework/console",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "~8.1.0",
        "joomla/application": "3.0.x-dev",
        "joomla/event": "3.0.x-dev",
        "joomla/string": "3.0.x-dev",
        "symfony/console": "^3.4|^4.4|^5.0"
    },
    "require-dev": {
        "joomla/test": "3.0.x-dev",
        "phpunit/phpunit": "^9.5.28",
        "psr/container": "^1.0",
        "squizlabs/php_codesniffer": "~3.7.1"
    },
    "suggest": {
        "psr/container-implementation": "To use the ContainerLoader"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Console\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-3.x-dev": "3.0-dev"
        }
    }
}
